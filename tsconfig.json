{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "lib": ["dom", "es6"],
      "allowJs": true,
      "jsx": "react",
      "declaration": true,
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "**/__tests__/*"]
  }
  